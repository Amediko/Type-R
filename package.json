{
  "name": "type-r",
  "version": "1.1.0-a18",
  "description": "Reactive serializable data layer for modern JS applications",
  "main": "./dist/index.js",
  "jsnext:main":"./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "dist",
    "lib",
    "src"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build-dist": "node_modules/.bin/rollup --config",
    "test": "node_modules/.bin/mocha tests/nestedtypes/functionality && npm run test:ts",
    "test:ts": "node_modules/.bin/mocha --opts tests/typescript/mocha.opts",
    "build": "node_modules/.bin/tsc && npm run build-dist",
    "docs": "node docs/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gaperton/TransactionalJS.git"
  },
  "keywords": [
    "model",
    "backbone",
    "data",
    "nestedtypes",
    "schema",
    "serialization"
  ],
  "author": "Vlad Balin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gaperton/TransactionalJS/issues"
  },
  "homepage": "https://github.com/gaperton/TransactionalJS#readme",
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.43",
    "chai": "^4.1.2",
    "handlebars": "^4.0.11",
    "highlight.js": "^9.12.0",
    "marked": "^0.3.6",
    "mocha": "^3.5.3",
    "reflect-metadata": "^0.1.10",
    "rollup": "^0.50.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "ts-node": "^3.3.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "tslib": "^1.8.0"
  }
}
